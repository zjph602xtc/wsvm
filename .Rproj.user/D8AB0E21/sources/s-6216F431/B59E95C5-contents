useDynLib("WSVM", .registration = TRUE, .fixes='C_')

import(graphics)
import(grDevices)
import(stats)

importFrom("methods", "as", "getClass", "new")
if(getRversion() >= "2.5.0")
    importFrom("utils", "write.table")

export(wsvm, write.wsvm, tune_wsvm, tune.control)
exportPattern("best\\..+")
S3method(coef, wsvm)
S3method(plot, wsvm)
S3method(plot, tune_wsvm)
S3method(predict, wsvm)
S3method(print, wsvm)
S3method(print, summary.wsvm)
S3method(print, tune_wsvm)
S3method(print, summary.tune_wsvm)
S3method(summary, wsvm)
S3method(summary, tune_wsvm)
S3method(wsvm, default)
S3method(wsvm, formula)
